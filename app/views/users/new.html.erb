<h1>Sign In or Sign Up Form</h1>
<div>


<%= form_for(@user) do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :password_digest %><br>
  <%= f.text_field :password_digest %><br>
  <%= f.submit %>
<% end %>
</div>
