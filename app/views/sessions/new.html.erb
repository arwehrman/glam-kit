<h1>Sign In</h1>
<div>
<%= form_tag ({controller: 'sessions', action: 'create', method: 'post'}) do %>

  <label>Username: </label>
  <%= text_field_tag 'user[email]' %><br><br>

  <label>Password: </label>
  <%= password_field_tag 'user[password]' %><br><br>

  <%= submit_tag "Sign In" %>

<% end %>
</div>
<%= link_to 'Home', '/' %><br>
<%= link_to 'Sign Up', new_user_path %><br>
